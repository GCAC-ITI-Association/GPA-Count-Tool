.view_titlebox{
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    overflow: hidden;
}

.image_gcacLogo{
    margin-top: 200rpx;
    width: 300rpx;
    height: 250rpx;
}

.view_title{
    margin: 50rpx;
    margin-bottom: 20rpx;
    font-size: 1.8rem;
    font-weight: bold;
}

.view_Join{
    
    border-radius: 10px;
    width: 90%;
    height: 100rpx;
    margin-top: 30rpx;

    display: flex;
    align-items: center;
    
}

.view_Join_Title{
    color: white;
    font-weight: bold;
    font-size: 1.2rem;

    margin-left: 40rpx;
    
}

.view_title_tip{
    font-size: 0.8rem;
    margin: 50rpx;
    margin-top: 0rpx;
    margin-bottom: 10rpx;
}